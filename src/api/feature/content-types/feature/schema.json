{
  "kind": "collectionType",
  "collectionName": "features",
  "info": {
    "singularName": "feature",
    "pluralName": "features",
    "displayName": "Feature",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "url_id": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "name": {
      "type": "string",
      "required": true
    },
    "body": {
      "type": "dynamiczone",
      "components": [
        "semantic.jumbo",
        "semantic.feature",
        "semantic.testimonial",
        "semantic.carousel",
        "semantic.feature-list",
        "semantic.rich-text"
      ],
      "required": true,
      "min": 1
    },
    "color": {
      "type": "enumeration",
      "enum": [
        "primary",
        "secondary",
        "tertiary",
        "accent",
        "red",
        "pink",
        "purple",
        "deep-purple",
        "indigo",
        "blue",
        "light-blue",
        "cyan",
        "teal",
        "green",
        "light-green",
        "lime",
        "yellow",
        "amber",
        "orange",
        "deep-orange",
        "brown",
        "grey",
        "blue-grey"
      ],
      "default": "primary",
      "required": true
    }
  }
}
