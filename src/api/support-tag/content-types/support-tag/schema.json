{
  "kind": "collectionType",
  "collectionName": "support_tags",
  "info": {
    "singularName": "support-tag",
    "pluralName": "support-tags",
    "displayName": "Support Tag",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "unique": true,
      "regex": "^[\\.a-zA-Z0-9-]+$"
    },
    "is_topic": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "support_articles": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::support-article.support-article",
      "inversedBy": "support_tags"
    }
  }
}
