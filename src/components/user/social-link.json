{
  "collectionName": "components_user_social_links",
  "info": {
    "displayName": "Social Link",
    "icon": "address-card"
  },
  "options": {},
  "attributes": {
    "url": {
      "type": "string",
      "required": true,
      "regex": "[-a-zA-Z0-9@:%._\\+~#=]{1,256}\\.[a-zA-Z0-9()]{1,6}\\b([-a-zA-Z0-9()@:%_\\+.~#?&\\/\\/=]*)"
    },
    "icon": {
      "type": "string",
      "required": true,
      "regex": "mdi-[a-z]+(-[a-z]+)*$"
    }
  }
}
